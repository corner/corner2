<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class table="many2many2AB" name="corner.demo.model.many2many2.AB">
    <cache usage="read-write"/>
    <many-to-one fetch="select" column="A" name="a" class="corner.demo.model.many2many2.A"/>
    <many-to-one fetch="select" column="B" name="b" class="corner.demo.model.many2many2.B"/>
  </class>
</hibernate-mapping>
