<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class table="many2many2AB" name="corner.demo.model.many2many2.AB">
    <cache usage="read-write"/>
    <id name="id">
      <generator class="uuid"/>
    </id>
    <many-to-one fetch="select" column="AId" name="a" class="corner.demo.model.many2many2.A"/>
    <many-to-one fetch="select" column="BId" name="b" class="corner.demo.model.many2many2.B"/>
    <property name="message"/>
    <property name="name"/>
  </class>
</hibernate-mapping>
