<module id="corner.translator" version="1.0.0"
	package="corner.orm.tapestry.translator">
	<!-- for trasnlator -->
	<contribution configuration-id="tapestry.form.translator.Translators">
		<bean name="vector" class="VectorTranslator"/>
		<bean name="num" class="NumTranslator"/>
		<bean name="exactdate" class="ExactDateTranslator"/>
		<bean name="simpledate" class="SimpleDateTranslator"/>
		<bean name="stringdate" class="StringDateTranslator"/>
		<bean name="timedate" class="TimeTranslator"/>
	</contribution>
</module>